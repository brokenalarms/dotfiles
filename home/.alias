# Just Ember
alias g="git"
alias et="yarn && ember test --server"
alias etm="yarn && ember test --path dist --server"
alias es="yarn && ember s"

# Mint
alias mu="mint update"
alias mrd="mint runDebug"
alias nukejs="yarn deleteDirs && yarn"

# Misc
killport() { lsof -ti:$1 | xargs kill -9 }

alias myip='dig +short `hostname` | tee >(pbcopy)'

